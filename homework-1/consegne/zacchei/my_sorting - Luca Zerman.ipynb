    "n = len(v)\n",
    "\n",
    "for i in range(n):\n",
    "    \n",
    "    for j in range(n-i-1):\n",
    "        \n",
    "            if v[j] > v[j+1]:\n",
    "                v[j], v[j+1] = v[j+1], v[j]\n",
    "\n",
    "print (v)"
